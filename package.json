{
  "name": "tauri-project-launcher",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"pnpm vite:dev\" \"pnpm devtools\"",
    "vite:dev": "cross-env DEBUG=0 vite",
    "devtools": "vue-devtools",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "test": "vitest",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "tauri": "tauri",
    "bump": "node bump-version.cjs",
    "check": "cargo check --manifest-path src-tauri/Cargo.toml"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "@vue/devtools": "^8.0.5",
    "@vueuse/core": "^14.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "fuse.js": "^7.1.0",
    "lucide-vue-next": "^0.562.0",
    "pinia": "^3.0.4",
    "radix-vue": "^1.9.17",
    "tailwind-merge": "^3.4.0",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify/types": "^2.0.0",
    "@pinia/testing": "^1.0.3",
    "@tailwindcss/vite": "^4.1.17",
    "@tauri-apps/cli": "^2.9.4",
    "@tsconfig/node22": "^22.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue": "^10.6.1",
    "lodash-es": "^4.17.21",
    "postcss": "^8.5.6",
    "prettier": "^3.7.1",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.2.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.14",
    "vue-tsc": "^3.1.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "@tailwindcss/oxide",
      "electron",
      "esbuild",
      "vue-demi"
    ]
  }
}
